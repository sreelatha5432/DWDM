MINIMUM;;;

diabetes<-read.csv("D:\\folders\\DWHDM\\diabetes.csv")

A<-c(diabetes$Age)

Minimum<-min(diabetes$Age)

MAXIMUM;;;

diabetes<-read.csv("D:\\folders\\DWHDM\\diabetes.csv")

A<-c(diabetes$Age)

Maximum<-max(diabetes$Age)

MEAN;;;

diabetes<-read.csv("D:\\diabetes.csv")

A<-c(diabetes$Age)

Mean<-mean(A)

MINMAX;;;

diabetes<-read.csv("D:\\folders\\DWHDM\\diabetes.csv")

A<-c(diabetes$Age)

MinMax<-(A-Minimum)/(Maximum-Minimum)

